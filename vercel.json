{
  "version": 2,
  "routes": [
    { "src": "/", "dest": "/src/OneUi_6.0_calc.html" },
    { "src": "/styles/(.*)", "dest": "/src/styles/$1" },
    { "src": "/scripts/(.*)", "dest": "/src/scripts/$1" },
    { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
    { "src": "/(.*)", "dest": "/src/$1" }
  ],
  "env": {
    "API_KEY": "$API_KEY"
  }
}